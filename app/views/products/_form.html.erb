<%= render('layouts/errors', :object => @product) %>

<%= form_for [@product] do |f| %>
  <p><%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :cost %>
  <%= f.text_field :cost %>
  <%= f.label :country %>
  <%= f.text_field :country %>
  <%= f.label :inventory %>
  <%= f.text_field :inventory %>
<%= f.submit %>
<% end %>
