<div class="container" id="products">
  <h2> <%= link_to "Back", root_path%></h2>
  <div class="jumbotron" id="productjumbo">
    <h1>Products</h1>
    <p><%= link_to "Add a New Product", new_product_path(@product) %></p>
  </div>
  <% @products.each do |product| %>
  <h3><%= link_to product.name, product_path(product)%> - $<%=product.cost%></h3>
  <h5>(Made in <%=product.country%>)</h5>
  <h5>Stock: <%=product.inventory%></h5>
  <hr>
  <% end %>
</div>
</div>
